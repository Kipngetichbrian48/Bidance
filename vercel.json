{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/**/*/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/price", "dest": "/api/price.js" },
    { "src": "/api/clear-cache", "dest": "/api/clear-cache.js" },
    { "src": "/api/orderbook", "dest": "/api/orderbook/[asset].js" },
    { "src": "/api/ohlc", "dest": "/api/ohlc/[asset]/[days].js" },
    { "src": "/api/kyc", "dest": "/api/kyc.js" }
  ]
}